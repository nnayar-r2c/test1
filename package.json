  },
  "scripts": {
    "analyze": "webpack-bundle-analyzer --port 4200 build/static/js/stats.json",
    "analyze:build": "ANALYZE=true yarn build && yarn analyze",
    "start": "ESLINT_NO_DEV_ERRORS=true rescripts start",
    "build": "tsc --noEmit && rescripts build",
    "test:local": "TZ=UTC rescripts test",
    "test:ci": "TZ=UTC rescripts test --maxWorkers=2 --silent --watchAll=false --coverage --ci --reporters=default --reporters=jest-junit",
    "test:coverage": "yarn test:local --watchAll=false --coverage --coverageReporters='text'",
    "test": "is-ci \"test:ci\" \"test:local\"",
    "eject": "rescripts eject",
    "preinstall": "node scripts/use-yarn.js",
    "prepare": "husky install",
    "lint": "yarn lint:css && yarn lint:ts",
    "lint:fix": "yarn lint:ts --fix && yarn lint:css --fix",
    "lint:js": "eslint \"./src/**/*.+(js|jsx)\" && yarn run tsc",
    "lint:ts-no-aggregate": "cross-env-shell NO_AGGREGATE=true \"yarn lint:ts\"",
    "lint:ts": "eslint -f scripts/aggregateEslintOutput.js \"./src/**/*.+(ts|tsx)\"",
    "lint:css": "stylelint \"./src/**/*.+(css|scss)\"",
    "lint:translations": "cp ./src/intl/en-US.ts ./src/intl/en-US.mjs; node scripts/lint-translations.mjs",
    "format": "prettier --write \"./src/**/*.+(js|jsx|ts|tsx)\"",
    "tsc": "tsc",
    "gc": "node scripts/generate-component",
    "delete-crowdin-branch-on-merge": "node scripts/deleteCrowdinBranchOnMerge.js"
  },
  "jest": {
    "resetMocks": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 50
      }
    }
  },
  "lint-staged": {
    "src/**/*.{scss,css}": [
      "yarn run lint:css --fix"
    ],
    "src/**/*.{js,jsx,json,ts,tsx}": [
      "prettier --write",
      "eslint --max-warnings=0"
    ]
  },
  "rescripts": [
    "config/webpack.rescripts",
    "config/webpackDevServer.rescripts"
  ]
}
